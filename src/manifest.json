{
  "name": "Extensions switcher",
  "manifest_version": 2,
  "browser_action": {
    "default_icon":  {
      "19": "img/icon_19.png",
      "38": "img/icon_38.png"
    },
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "img/icon_16.png",
    "48": "img/icon_48.png",
    "128": "img/icon_128.png"
  },
  "offline_enabled": true,
  "permissions": [ "storage", "management" ],
  "description": "__MSG_appDesc__",
  "default_locale": "en",
  "version": "1.0.0"
}